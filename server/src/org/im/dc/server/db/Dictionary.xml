<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.im.dc.server.db.DoDictionary">
    <select id="getDictionaries" resultType="RecDictionary" resultMap="RecDictionaryMap">
        SELECT DISTINCT dict, val
        FROM Dictionaries
    </select>
    <insert id="addDictionaries" parameterType="java.util.List" >
        INSERT INTO Dictionaries (dict,val)
        VALUES
        <foreach collection="items" item="item" index="index" separator=",">
            (#{item.dict},#{item.val})
        </foreach>
        <!-- ON CONFLICT DO NOTHING -->
    </insert>

    <resultMap id="RecDictionaryMap" type="RecDictionary">
        <result property="dict" column="dict" />
        <result property="val" column="val" />
    </resultMap>
</mapper>
