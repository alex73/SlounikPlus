						<xs:appinfo>Граматычная памета</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="desc" type="xs:string" maxOccurs="1" minOccurs="1">
					<xs:annotation>
						<xs:appinfo>Тэкст тлумачэння</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="ex" type="xs:string" maxOccurs="unbounded" minOccurs="0">
					<xs:annotation>
						<xs:appinfo>Прыклад</xs:appinfo>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
